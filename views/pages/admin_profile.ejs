<script>
function checkForm(form)
{
    if(form.name.value == "") {
        alert("Enter Name!");
          form.name.focus();
          return false;
    }
	
	if(form.mobile.value.length != 10) {
		alert("Enter valid Mobile No.");
		form.mobile.focus();
		return false;
	}
	if(form.email.value == "") {
		alert("Enter Email-Id");
		form.email.focus();
		return false;
	}
	re =  /\S+@\S+\.\S+/;
	if(!re.test(form.email.value)) {
		alert("Enter valid Email-Id");
		form.email.focus();
		return false;
	}
	
}

